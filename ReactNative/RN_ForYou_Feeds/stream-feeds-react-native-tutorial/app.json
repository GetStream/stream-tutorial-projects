{
  "expo": {
    "name": "Stream Feeds Tutorial",
    "slug": "stream-feeds-react-native-tutorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "stream-feeds",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.getstream.feeds.tutorial"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2563EB"
      },
      "package": "io.getstream.feeds.tutorial"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them in posts."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
